{
    "title": "牟梦云的Linux学习笔记",
    "author": "牟梦云",

    "links" : {
        "sidebar" : {
            "Home" : "https://www.24linux.com"
        }
    },

    "plugins": [ "toggle-chapters", "github", "github-buttons", "terminal", "-lunr", "-search", "search-plus" ],

    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },

        "github": {
            "url": "https://github.com/mumengyun"
        },

        "github-buttons": {
            "repo": "mumengyun/linux",
            "types": [
                "star",
                "watch",
                "fork"
            ],
            "size": "small"
        },

        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        }
    }
}